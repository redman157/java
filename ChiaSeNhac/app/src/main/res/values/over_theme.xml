        <item name="indicator_edit">@color/light_green</item>
        <item name="progress_bar_enable">@color/green_A400</item>
        <item name="progress_bar_unenable">@color/white</item>
        <item name="thumb">@color/light_green</item>
        <item name="popup_text_color">@color/light</item>

        <item name="color_primary">@color/light_green</item>
        <item name="color_primary_dark">@color/green</item>
        <item name="accent_color">@color/green_A400</item>
    </style>

