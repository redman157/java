    <style name="OverlayThemeWhite" parent="AppTheme">
        <!--khi button choose-->
        <item name="select_enable">@color/red_A400</item>
        <!--backgroud sliding panel-->
        <item name="back_ground_sliding">@color/blackOpaque</item>
        <!--back ground toàn app-->
        <item name="back_ground_app">@color/light</item>
        <!--màu chữ của title playlist, music-->
        <item name="back_ground_title">@color/black</item>
        <!--back gorund của dialog -->
        <item name="back_ground_dialog">@color/white</item>
        <!--back ground cardview-->
        <item name="back_ground_button">@color/light_grey</item>
        <!--background chữ-->
        <item name="text_color">@color/black</item>
        <!--background của nút -->
        <item name="tint_color">@color/white</item>
        <!--indicator của editetxt-->
        <item name="indicator_edit">@color/light_grey</item>
        <!--progress đang chạy -->
        <item name="progress_bar_enable">@color/white</item>
        <!--progres con lại-->
        <item name="progress_bar_unenable">@color/blackOpaque</item>
        <!--cục thub của seekbar-->
        <item name="thumb">@color/grey</item>
        <!--cuộn của recycler view-->
        <item name="popup_text_color">@color/white</item>

        <item name="color_primary">@color/light_grey</item>
        <item name="color_primary_dark">@color/blackOpaque</item>
        <item name="accent_color">@color/white</item>
    </style>

